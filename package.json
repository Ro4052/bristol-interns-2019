{
  "name": "bristol-interns-19",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "classnames": "^2.2.6",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "express-socket.io-session": "^1.3.5",
    "express-ws": "^4.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "socket.io": "^2.2.0",
    "socket.io-mock": "^1.2.3"
  },
  "scripts": {
    "start": "node server.js",
    "start:devserver": "nodemon server.js --watch ./server --watch ./server.js",
    "start:devclient": "react-scripts start",
    "start:testserver": "concurrently --success first --kill-others \"npx cross-env PORT=12345 NODE_ENV=testing node server.js\" \"npx cross-env PORT=12346 NODE_ENV=testing node ./server/proxy/server.js\"",
    "build": "react-scripts build",
    "test": "npm run test:client && npm run test:e2e",
    "test:client": "react-scripts test --coverage --watchAll=false",
    "test:e2e": "concurrently --success first --kill-others \"npm run start:testserver\" \"node run-cypress-test.js -r tap\"",
    "test:e2eUI": "concurrently --success first --kill-others \"npm run start:testserver\" \"cypress open\"",
    "cypress:open": "cypress open",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app",
    "env": {
      "cypress/globals": true
    },
    "plugins": [
      "cypress"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "axios-cookiejar-support": "^0.5.0",
    "concurrently": "^4.1.1",
    "cypress": "^3.4.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-plugin-cypress": "^2.2.1",
    "http-proxy-middleware": "^0.19.1",
    "moxios": "^0.4.0",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1",
    "redux-mock-store": "^1.5.3",
    "socket.io-client": "^2.2.0",
    "start-server-and-test": "^1.9.1",
    "tough-cookie": "^3.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/components/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/"
    ]
  }
}
